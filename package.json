{
  "name": "leyla-parser",
  "version": "1.0.0",
  "description": "Leyla - автоматический парсер вакансий для Vakansiya.az",
  "private": true,
  "scripts": {
    "dev": "vercel dev",
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "parse:boss": "node -r ts-node/register scripts/parse-boss.ts",
    "parse:ejob": "node -r ts-node/register scripts/parse-ejob.ts",
    "parse:all": "npm run parse:boss && npm run parse:ejob"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "cheerio": "^1.0.0-rc.12",
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@vercel/node": "^3.0.11",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
